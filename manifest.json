{
  "name": "My First Extension - MAYANK",
  "description": "Change the current page color",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png"
  },
  "chrome_url_overrides" : {
        "newtab": "background.html"
    },
  "manifest_version": 2
}
